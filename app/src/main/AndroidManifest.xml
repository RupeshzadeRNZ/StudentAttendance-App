<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudentAttend"
        tools:targetApi="31">
        <activity
            android:name=".activitys.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713" />

        <activity
            android:name=".activitys.CreateAccount"
            android:exported="false" />
        <activity
            android:name=".activitys.LogIn"
            android:exported="false">

        </activity>
        <activity
            android:name=".activitys.Update_Data_dialog"
            android:exported="false" />
        <activity
            android:name=".activitys.StudentDetails"
            android:exported="false" />
        <activity
            android:name=".activitys.AddAttendance"
            android:exported="false" />
        <activity
            android:name=".activitys.SearchStudent"
            android:exported="false" />
        <activity
            android:name=".activitys.InsterData"
            android:exported="false" />
        <activity
            android:name=".activitys.MainActivity"
            android:exported="true">

        </activity>
    </application>

</manifest>